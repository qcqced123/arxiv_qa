{
    "pipeline_setting": {
        "pipeline_type": "make",
        "work_flow_state": "resume",
        "n_jobs": 4,
        "resume": false,
        "task": "CasualLanguageModel"
    },

    "generate_options": {
        "model_name": "microsoft/Phi-3-mini-128k-instruct",
        "tokenizer_name": "microsoft/Phi-3-mini-128k-instruct",
        "max_len": 131072,
        "max_new_tokens": 1024,
        "return_full_text": false,
        "strategy": "beam",
        "num_beams": 5,
        "temperature": 0.7,
        "top_k": 100,
        "top_p": 0.9,
        "penalty_alpha": 1.0,
        "repetition_penalty": 1.0,
        "length_penalty": 2.0,
        "no_repeat_ngram_size": 4,
        "do_sample": true,
        "use_cache": true,
        "use_pretrained": true,
        "generate_mode": true,
        "hub": "huggingface",
        "quantization": true,
        "lora": false,
        "qlora": false,
        "prompt_tuning": false,
        "layer_norm_eps": 1e-7,
        "attention_probs_dropout_prob": 0.1,
        "hidden_dropout_prob": 0.1,
        "init_weight": "kaiming_normal",
        "initializer_range": 0.02
    },

    "common_settings": {
        "wandb": true,
        "seed": 42,
        "n_gpu": 1,
        "gpu_id": 0,
        "num_workers": 4
    }
}
